{
  "name": "just-i",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "just",
    "justfile",
    "install"
  ],
  "license": "Unlicense",
  "author": "Alex Mullen <alex@xela.foo>",
  "type": "commonjs",
  "bin": {
    "just": "dist/exec.js"
  },
  "scripts": {
    "test": "rm -rf lib/bin && env JUST_I_DEBUG=true node lib/exec.js -f __test__/justfile",
    "pretest:dist": "pnpm run build",
    "test:dist": "rm -rf dist/bin && node dist/exec.js -f __test__/justfile",
    "build": "node __bin__/build.js",
    "latest": "node __bin__/latest.js",
    "lint": "biome lint .",
    "fix": "biome lint --write .",
    "fmt": "dprint fmt",
    "clean": "rm -rf dist",
    "install": "pnpm run build",
    "postinstall": "node __bin__/postinstall.js",
    "prepublishOnly": "pnpm test:dist"
  },
  "dependencies": {
    "node-uname": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@octokit/rest": "^21.0.1",
    "decompress": "^4.2.1",
    "dprint": "^0.47.2",
    "esbuild": "^0.23.0"
  },
  "engine": {
    "node": ">= 12"
  },
  "packageManager": "pnpm@9.5.0"
}
